{
  "name": "Simple书签",
  "description": "在新标签页浏览你的书签。",
  "manifest_version": 2,
  "version": "0.1",
  "icons": {
    "32": "img/icon32.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/append.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/append.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "offline_enabled": true,
  "permissions": [
    "bookmarks",
    "storage",
    "tabs",
    "history"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "web_accessible_resources": [
    "img/right.png",
    "img/down.png"
  ],
  "chrome_url_overrides": {
    "newtab": "bookMark.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}